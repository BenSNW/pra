<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="PRA" basedir="." default="jar">

    <path id="classpath">
        <pathelement location="lib/graphchi-java-0.2.jar"/>
        <pathelement location="lib/guava-13.0.1.jar"/>
        <pathelement location="lib/mallet.jar"/>
        <pathelement location="lib/log4j-1.2.16.jar"/>
    </path>

    <target name="clean" description="Delete all generated files">
        <delete dir="classes"/>
    </target>

    <target name="compile" description="Compiles the Task">
        <mkdir dir="classes"/>
        <javac srcdir="src" debug="true" destdir="classes" classpathref="classpath"/>
    </target>

</project>
